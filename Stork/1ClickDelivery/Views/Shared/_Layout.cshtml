                            var isAdmin = Roles.IsUserInRole("Admin");
                            <li class="dropdown hidden-xs hidden-sm">
                                <a href="" class="dropdown-toggle" data-toggle="dropdown">
                                    <img id="account" src="~/img/usericon.png" style="width:30px;height:auto" />
                                    <img id="accounthov" src="~/img/usericonhov.png" style="width:30px;height:auto" />  Account  <b class="caret"></b>
                                </a>
                                <ul class="dropdown-menu">
                                    @if (isAdmin)
                                    {

                                        <li>
                                            @Html.ActionLink("Dashboard", "Index", "Dashboard", routeValues: null, htmlAttributes: new { id = "adminLink" })
                                        </li>

                                    }
                                    else
                                    {
                                        <li>
                                            @Html.ActionLink("Dashboard", "Index", "Dashboard", routeValues: null, htmlAttributes: new { id = "dashboardLink" })
                                        </li>
                                    }
                                    <li>

                                        @using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm", @class = "navbar-form", style = "padding-top: 4px;" }))
                                        {

                                            @Html.AntiForgeryToken()
                                            <a href="javascript:document.getElementById('logoutForm').submit()">  <span class="glyphicon glyphicon-log-out"></span> Log off</a>
                                        }
                                    </li>
                                </ul>
                            </li>
