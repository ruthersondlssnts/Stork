    <script>
   
            
            $('.closea').click(function () {
                $("#announcement").css("display", "none");
            });
      
       


        function removeEffectOnNavbar() {
            $(window).unbind('scroll');
            $(".navbar-fixed-top").addClass("top-nav-collapse");
            $(".top-area").addClass("top-padding");
            $(".navbar-brand").addClass("reduce");
            $(".navbar-custom ul.nav ul.dropdown-menu").css("margin-top", "11px");
        }
        $("#m2").click(function () {
            $(".navbar-main-collapse").removeClass("in");
        }
        );
        $("#m1").click(function () {
            $(".navbar-main-collapse1").removeClass("in");
        }
        );
        $(".navbar-main-collapse ul li").mouseup(function () {
            $(".navbar-main-collapse1").removeClass("in");
            $(".navbar-main-collapse2").removeClass("in");
        }
        );

        $(document).ready(function () {
            var currentController = '@ViewContext.RouteData.Values["controller"].ToString()';
            if (currentController === "Home" || currentController === "Tracking" ) {
                $("#navbarCollapse").remove();
            }
        });
    </script>
    @RenderSection("scripts", required: false)



